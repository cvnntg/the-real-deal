package environment;

import javax.swing.*;

import app.*;
import visual.*;
import bot.FrontBot;

public class DigiEnvironment extends AbstractMultimediaApp
{
	FrontBot			bot;
	JPanel				contentPane;
	Visualization		visualization;
	VisualizationView	component;
	
	public DigiEnvironment()
	{
		visualization = new Visualization();
		
		bot = new FrontBot("justFace.png");
		visualization.add(bot);
		
		component = visualization.getView();
		contentPane = (JPanel)rootPaneContainer.getContentPane();
		component.setBounds(0, 0, 500, 500);
		contentPane.add(component);
	}
	
	
	
	
	
}
